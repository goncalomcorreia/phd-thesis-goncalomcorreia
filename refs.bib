@inproceedings{belinkov2017neural,
  title     = {What do Neural Machine Translation Models Learn about Morphology?},
  author    = {Belinkov, Yonatan and Durrani, Nadir and Dalvi, Fahim and Sajjad, Hassan and Glass, James},
  booktitle = {Proceedings of the 55th Annual Meeting of the Association for Computational Linguistics (Volume 1: Long Papers)},
  volume    = {1},
  pages     = {861--872},
  year      = {2017},
  note      = {Various NMT systems are trained with translation to, from, and between morphologically rich languages (but none that are agglutinative). Word representations output by encoders are used as input to non-linear classifiers for predicting POS and morphological tags. Models with CNNs over characters do better than word-level models. POS/morphology information mostly expressed at lower model layers. Classifier results are better when the target language is morphologically poor, although there is no significance test. Could it be that subword structure is more important if it needs to be translated into more independent words, as is the case when you translate to a morphologically-poor language? Or is it just that English is an easy target? The decoder does not learn morphologically-aware word representations (except for a little bit if there is no attention). Using character-based word reps does not help the decoder learn morphology. This paper is interesting, but does not give much of a baseline for how easy it is to get good morph/POS results.}
}

